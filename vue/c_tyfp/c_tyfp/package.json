  "main": "webpack/webpack.server.js",
  "scripts": {
    "pc_dev": "cross-env projectName=pc webpack-dev-server --mode development --hot --progress --config ./webpack/webpack.server.js",
    "pc_build": "cross-env projectName=pc webpack --progress --mode production --config ./webpack/webpack.server.js",
    "pc_start": "npm run pc_dev",
    "mobile_dev_only": "cross-env projectName=mobile webpack-dev-server --mode development --hot --progress --config ./webpack/webpack.server.js",
    "mobile_dev": "cross-env projectName=mobile webpack-dev-server --mode development --hot --progress --config ./webpack/webpack.server.js &",
    "mobile_build": "cross-env projectName=mobile webpack --progress --mode production --config ./webpack/webpack.server.js",
    "mobile_start": "npm run mobile_dev_only",
    "all_build": "npm run mobile_build && npm run pc_build",
    "all_start": "npm run mobile_dev && npm run pc_dev",
    "server": "cross-env NODE_ENV=local node server/app.js",
    "check": "eslint --ext .js src/ --cache",
    "lint": "eslint --cache --fix src"
  },
